{
    "name": "@openship/nextjs",
    "version": "0.0.2",
    "description": "An implementation of the openship protocol for Next.js",
    "scripts": {
      "build": "cargo build --target wasm32-wasip1 --release",
      "copy-wasm": "mkdir -p dist && cp target/wasm32-wasip1/release/openship_nextjs_swc.wasm dist/",
      "prepare": "npm run build && npm run copy-wasm"
    },
    "files": [
      "dist/openship_nextjs_swc.wasm"
    ],
    "keywords": ["swc", "plugin", "nextjs", "wasm", "openship"],
    "author": "Gilles Pirio",
    "license": "MIT",
    "repository": {
      "type": "git",
      "url": "git+https://github.com/gip/openship"
    },
    "devDependencies": {
      "@swc/core": "^1.3.0"
    },
    "peerDependencies": {
      "next": "15.0.0-canary.179"
    }
  }
